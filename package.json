{
  "name": "mevn_stack",
  "version": "1.0.0",
  "engines": {
    "node": "14.x"
  },
  "description": "Simple web application to handle authentication with vue.js",
  "main": "app.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node app.js",
    "server": "nodemon app.js",
    "client": "npm run serve --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Habibib",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.39.0",
    "aws-sdk": "^2.1018.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.16.4",
    "express-list-endpoints": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.12",
    "multer": "^1.4.3",
    "multer-s3": "^2.10.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0"
  }
}
